{
  "name": "@wqbot/cli",
  "version": "0.1.0",
  "description": "WQBot CLI - Command line interface for the AI butler",
  "type": "module",
  "bin": {
    "wqbot": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@wqbot/core": "workspace:*",
    "@wqbot/models": "workspace:*",
    "@wqbot/skills": "workspace:*",
    "@wqbot/storage": "workspace:*",
    "@wqbot/security": "workspace:*",
    "@wqbot/backend": "workspace:*",
    "commander": "^12.0.0",
    "ink": "^4.4.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "react": "^18.2.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
