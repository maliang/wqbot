{
  "name": "@wqbot/storage",
  "version": "0.1.0",
  "description": "WQBot storage - SQLite database, conversation history, and settings",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@wqbot/core": "workspace:*",
    "sql.js": "^1.10.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
